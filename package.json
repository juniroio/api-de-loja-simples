{
  "name": "express1",
  "version": "1.0.0",
  "description": "Um projeto simples com express",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon -e yaml,js,json --exec node src/index.js",
    "start": "npx sequelize-cli db:migrate && cross-env NODE_ENV=production node src/index.js",
    "migrate-dev": "cross-env NODE_ENV=development npx sequelize-cli db:migrate",
    "migrate-prod": "cross-env NODE_ENV=production npx sequelize-cli db:migrate"
  },
  "author": "Juniro",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.7"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "better-sqlite3": "^11.5.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.21.1",
    "express-ejs-layouts": "^2.5.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.11.3",
    "node": "^22.10.0",
    "sequelize": "^6.37.4",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^10.0.0",
    "yamljs": "^0.3.0"
  }
}
